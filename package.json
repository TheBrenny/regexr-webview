{
	"name": "regexr-webview",
	"displayName": "RegExr (WebView)",
	"description": "Opens a WebView panel to https://regexr.com",
	"publisher": "JustBrenny",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/TheBrenny/regexr-webview.git"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:regexr-webview",
		"onCommand:regexr-webview.openRegexr"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "regexr-webview.openRegexr",
				"title": "Open RegExr",
				"category": "RegExr",
				"icon": "$(regex)"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
